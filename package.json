{
  "name": "messaging-app",
  "version": "1.0.0",
  "description": "web based real-time instant messaging application",
  "main": "server/index.js",
  "scripts": {
    "server-start": "node server/index.js",
    "server-dev": "nodemon server/index.js",
    "client-serve": "webpack serve --mode=development --config ./webpack.config.js --open",
    "client-build": "webpack --mode=production",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/febriadj/messaging-app.git"
  },
  "keywords": [
    "instant-messaging",
    "real-time-app",
    "web-based"
  ],
