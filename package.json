  "license": "MIT",
  "bugs": {
    "url": "https://github.com/febriadj/messaging-app/issues"
  },
  "homepage": "https://github.com/febriadj/messaging-app#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-wildcard": "^7.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
